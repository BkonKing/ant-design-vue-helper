{
	"a-button/disabled": {
		"description": "按钮失效状态",
		"optionType": "boolean",
		"defaultValue": "false"
	},
	"a-button/ghost": {
		"description": "幽灵属性，使按钮背景透明",
		"optionType": "boolean",
		"defaultValue": "false"
	},
	"a-button/htmlType": {
		"description": "设置 button 原生的 type 值，可选值请参考 HTML 标准",
		"optionType": "string",
		"defaultValue": "button"
	},
	"a-button/icon": {
		"description": "设置按钮的图标类型",
		"optionType": "string",
		"defaultValue": "-"
	},
	"a-button/loading": {
		"description": "设置按钮载入状态",
		"optionType": "boolean | { delay: number }",
		"defaultValue": "false"
	},
	"a-button/shape": {
		"options": ["circle", "round"],
		"description": "设置按钮形状，可选值为 circle、 round 或者不设",
		"optionType": "string",
		"defaultValue": "-"
	},
	"a-button/size": {
    "options": ["small", "large"],
		"description": "设置按钮大小，可选值为 small large 或者不设",
		"optionType": "string",
		"defaultValue": "default"
	},
	"a-button/type": {
    "options": ["primary", "dashed", "danger", "link"],
		"description": "设置按钮类型，可选值为 primary dashed danger link 或者不设",
		"optionType": "string",
		"defaultValue": "default"
	},
	"a-button/block": {
		"description": "将按钮宽度调整为其父宽度的选项",
		"optionType": "boolean",
		"defaultValue": "false"
	}
}